{
  "name": "kate_site",
  "version": "1.0.0",
  "description": "kate lab website",
  "main": "_src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run-script clean:project && webpack --env=dev --progress --profile --colors && concurrently \"webpack-dev-server --env=dev\" \"bundle exec jekyll serve --trace\"",
    "build": "npm run-script clean:project && NODE_ENV=production webpack --env=prod --progress --profile --colors && JEKYLL_ENV=production bundle exec jekyll build",
    "deploy": "npm run-script clean:docs && npm run-script build && cp -r _site/ docs/ && cp -r assets docs && touch docs/.nojekyll && git add ./docs && git commit -m \"deploy site\" && git push",
    "clean:docs": "rimraf ./docs",
    "clean:project": "rimraf ./_site ./assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sneakers-the-rat/kate_site.git"
  },
  "author": "jonny saunders",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sneakers-the-rat/kate_site/issues"
  },
  "homepage": "https://github.com/sneakers-the-rat/kate_site#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "html-webpack-plugin": "^3.2.0"
  }
}
